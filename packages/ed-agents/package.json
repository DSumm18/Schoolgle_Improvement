{
  "name": "@schoolgle/ed-agents",
  "version": "0.1.0",
  "description": "Agentic framework for Ed chatbot - specialist personas with qualified expertise",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./agents": "./dist/agents/index.js",
    "./orchestrator": "./dist/orchestrator/index.js",
    "./guardrails": "./dist/guardrails/index.js",
    "./perspectives": "./dist/perspectives/index.js",
    "./skills": "./dist/skills/index.js",
    "./knowledge-base": "./dist/knowledge-base/index.js",
    "./models": "./dist/models/index.js",
    "./credit": "./dist/credit/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^3.0.5"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "keywords": [
    "ai",
    "agents",
    "chatbot",
    "education",
    "school"
